executable = /bin/bash
arguments = calc_input0.sh file0.in file0.out

should_transfer_files = YES
transfer_input_files = calc_input0.sh, file0.in

transfer_output_files = file0.out
when_to_transfer_output = ON_EXIT

log = $(JobBatchName).log
output = $(JobBatchName).out
error = $(JobBatchName).err

JobBatchName = CalcInput0

request_cpus = 1
request_memory = 32MB
request_disk = 512MB

queue

