executable = /bin/bash
arguments = calc_input1.sh file1.in file1.out

should_transfer_files = YES
transfer_input_files = calc_input1.sh, file1.in

transfer_output_files = file1.out
when_to_transfer_output = ON_EXIT

log = $(JobBatchName).log
output = $(JobBatchName).out
error = $(JobBatchName).err

JobBatchName = CalcInput1

request_cpus = 1
request_memory = 32MB
request_disk = 512MB

queue

