executable = /bin/bash
arguments = calc_input.sh file$(num).in file$(num).out

should_transfer_files = YES
transfer_input_files = calc_input.sh, file$(num).in

transfer_output_files = file$(num).out
when_to_transfer_output = ON_EXIT

log = $(JobBatchName).log
output = $(JobBatchName).out
error = $(JobBatchName).err

JobBatchName = CalcInput$(num)

request_cpus = 1
request_memory = 32MB
request_disk = 512MB

queue num in (0 1 2)
