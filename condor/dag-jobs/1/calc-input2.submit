executable = /bin/bash
arguments = calc-input2.sh file2.in file2.out

should_transfer_files = YES
transfer_input_files = calc-input2.sh, file2.in

transfer_output_files = file2.out
when_to_transfer_output = ON_EXIT

log = $(JobBatchName).log
output = $(JobBatchName).out
error = $(JobBatchName).err

JobBatchName = CalcInput2

request_cpus = 1
request_memory = 32MB
request_disk = 512MB

queue
